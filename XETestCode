        START   4000    .This is a comment
        TIXR
Label   CLEAR   X
        CLEAR   A
        LDCH    =X’00’
        RMO     A,S
        +LDT    #1024
$AALOOP TD      =X’F1’
        JEQ     $AALOOP
        RD      =X’F1’
        COMPR   A,S
        JEQ     $AAEXIT
        STCH    BUFFER,X
        TIXR    T
        JLT     $AALOOP
$AAEXIT STX     LENGTH
        END
